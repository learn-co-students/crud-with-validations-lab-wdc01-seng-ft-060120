<h2>All Songs</h2>
<table>
<thead>  <tr>
    <td>Title</td>
    <td>Artist</td>
    <td>Release Year</td>
    <td>Released?</td>
    <td>Genre</td>
  </tr> </thead>
  
  <%@songs.each do |s| %>
    <tbody><tr>
      <td><%= link_to s.title, song_path(s)%></td>
      <td><%= s.artist_name%></td>
      <td><%= s.release_year%></td>
      <td><%= s.released%></td>
      <td><%= s.genre%></td>
    </tr></tbody>
  <%end%>
</table>
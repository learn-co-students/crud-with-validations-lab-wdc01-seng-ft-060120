<h1><%= @song.title %></h1>
<p>
    <strong>Artist: </strong><%= @song.artist_name %>
<p>
<% if @song.released? %>
  <p>
    Released in
    <strong><%= @song.release_year %></strong>
  </p>
<% end %>

<p>
  <strong>Genre:</strong>
  <%= @song.genre %>
</p>

<%= link_to 'Edit', edit_song_path(@song) %>